Module:
-------
  name: quicksort
  imports: Prelude

  types:
  ------

  functions:
  ----------
    qsort:
      $1 <- LHS[1]
      case $1 of Prelude.[] -> return (Prelude.[])
                 Prelude.:  -> $2 <- $1[1]
                               $3 <- $1[2]
                               return (Prelude.++ ((quicksort.qsort ((Prelude.filter (((Prelude.flip ((Prelude.<)) $2))) $3)))) ((Prelude.: $2 ((quicksort.qsort ((Prelude.filter (((Prelude.flip ((Prelude.>=)) $2))) $3)))))))
      return $1
    goal:
      return (quicksort.qsort ((Prelude.: 2 ((Prelude.: 3 ((Prelude.: 1 ((Prelude.: 0 (Prelude.[]))))))))))