  name: rev
  imports: Prelude

  types:
  ------

  functions:
  ----------
    append:
      $1 <- LHS[1]
      $2 <- LHS[2]
      case $1 of Prelude.[] -> return $2
                 Prelude.:  -> $3 <- $1[1]
                               $4 <- $1[2]
                               return (Prelude.: $3 ((rev.append $4 $2)))
      return $1
    rev:
      $1 <- LHS[1]
      case $1 of Prelude.[] -> return (Prelude.[])
                 Prelude.:  -> $2 <- $1[1]
                               $3 <- $1[2]
                               return (rev.append ((rev.rev $3)) ((Prelude.: $2 (Prelude.[]))))
      return $1
    goal1:
      return (rev.append ((Prelude.: 1 ((Prelude.: 2 (Prelude.[]))))) ((Prelude.: 3 ((Prelude.: 4 (Prelude.[]))))))
    goal2:
      return (rev.rev ((Prelude.: 1 ((Prelude.: 2 ((Prelude.: 3 ((Prelude.: 4 (Prelude.[]))))))))))
    goal3:
      return (rev.rev ((Prelude.: 1 ((Prelude.: 2 ((Prelude.: 3 ((Prelude.: 4 ((Prelude.: 5 ((Prelude.: 6 ((Prelude.: 7 ((Prelude.: 8 ((Prelude.: 9 ((Prelude.: 10 (Prelude.[]))))))))))))))))))))))