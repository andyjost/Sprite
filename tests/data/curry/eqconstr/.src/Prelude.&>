def step(lhs):
  _1 = lhs[0]
  _2 = lhs[1]
  selector = hnf(lhs, p_1, typedef=switch_type).info.tag
  if selector == 1:
    lhs.Node(ni__Fwd, _2)
    return
  elif selector == 0:
    lhs.Node(ni__Failure)
    return
  else:
    lhs.Node(ni__Failure)
    return


