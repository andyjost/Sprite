  function "digit.goal" 2
    code
      declare_var 1 ILhs (("digit","goal"),1)
      declare_var 2 ILhs (("digit","goal"),2)
      return
        Node "Prelude.&" (
          Node "Prelude.=:=" (
            Node "Prelude._impl#+#Prelude.Num#Prelude.Int" (
              reference_var 1 ,
              reference_var 1 ) ,
            reference_var 2 ) ,
          Node "Prelude.&" (
            Node "Prelude.=:=" (
              Node "Prelude._impl#*#Prelude.Num#Prelude.Int" (
                reference_var 1 ,
